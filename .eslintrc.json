{
	"extends": "google",
	"env": {
		"es6": true,
		"node": true,
		"mocha": true
	},
	"rules": {
		"camelcase": ["error", {
			"properties": "never"
		}],
		"require-jsdoc": ["error", {
			"require": {
				"FunctionDeclaration": false,
				"MethodDefinition": false,
				"ClassDeclaration": false
			}
		}]
	}
}
